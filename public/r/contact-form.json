{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "contact-form",
  "type": "registry:block",
  "title": "Contact Form",
  "description": "Professional contact form with name, email, subject, and message fields",
  "dependencies": [],
  "registryDependencies": [
    "button",
    "card",
    "input",
    "label",
    "text",
    "textarea",
    "select",
    "icons"
  ],
  "files": [
    {
      "path": "registry/new-york/blocks/contact-form.tsx",
      "content": "'use client'\r\n\r\nimport { Button } from '@/registry/new-york/ui/button'\r\nimport {\r\n  Card,\r\n  CardContent,\r\n  CardDescription,\r\n  CardHeader,\r\n  CardTitle,\r\n} from '@/registry/new-york/ui/card'\r\nimport { Input } from '@/registry/new-york/ui/input'\r\nimport { Label } from '@/registry/new-york/ui/label'\r\nimport { Text } from '@/registry/new-york/ui/text'\r\nimport { Textarea } from '@/registry/new-york/ui/textarea'\r\nimport {\r\n  Select,\r\n  SelectContent,\r\n  SelectItem,\r\n  SelectTrigger,\r\n  SelectValue,\r\n} from '@/registry/new-york/ui/select'\r\nimport { AlignCenterIcon } from '@/registry/new-york/icons'\r\nimport { MailCheckIcon } from '@/registry/new-york/icons/mail-check'\r\n\r\nexport function ContactForm() {\r\n  return (\r\n    <div className=\"container mx-auto flex min-h-[700px] items-center justify-center p-4\">\r\n      <Card className=\"w-full max-w-2xl\">\r\n        <CardHeader>\r\n          <CardTitle>\r\n            <Text width=\"lg\" emphasis=\"primary\" />\r\n          </CardTitle>\r\n          <CardDescription>\r\n            <Text width=\"full\" color=\"muted\" />\r\n          </CardDescription>\r\n        </CardHeader>\r\n        <CardContent className=\"space-y-6\">\r\n          <div className=\"grid gap-4 md:grid-cols-2\">\r\n            <div className=\"space-y-2\">\r\n              <Label htmlFor=\"contact-name\">\r\n                <Text width=\"xs\" />\r\n              </Label>\r\n              <Input\r\n                id=\"contact-name\"\r\n                skeleton\r\n                skeletonIcon={<AlignCenterIcon size={16} className=\"text-muted-foreground\" />}\r\n              />\r\n            </div>\r\n            <div className=\"space-y-2\">\r\n              <Label htmlFor=\"contact-email\">\r\n                <Text width=\"xs\" />\r\n              </Label>\r\n              <Input\r\n                id=\"contact-email\"\r\n                skeleton\r\n                skeletonIcon={<MailCheckIcon size={16} className=\"text-muted-foreground\" />}\r\n              />\r\n            </div>\r\n          </div>\r\n          <div className=\"space-y-2\">\r\n            <Label htmlFor=\"subject\">\r\n              <Text width=\"xs\" />\r\n            </Label>\r\n            <Select>\r\n              <SelectTrigger id=\"subject\">\r\n                <SelectValue>\r\n                  <Text width=\"md\" />\r\n                </SelectValue>\r\n              </SelectTrigger>\r\n              <SelectContent>\r\n                <SelectItem value=\"general\">\r\n                  <Text width=\"sm\" />\r\n                </SelectItem>\r\n                <SelectItem value=\"support\">\r\n                  <Text width=\"sm\" />\r\n                </SelectItem>\r\n                <SelectItem value=\"sales\">\r\n                  <Text width=\"sm\" />\r\n                </SelectItem>\r\n              </SelectContent>\r\n            </Select>\r\n          </div>\r\n          <div className=\"space-y-2\">\r\n            <Label htmlFor=\"message\">\r\n              <Text width=\"xs\" />\r\n            </Label>\r\n            <Textarea id=\"message\" skeleton skeletonLines={5} />\r\n          </div>\r\n          <div className=\"flex justify-end gap-2\">\r\n            <Button variant=\"outline\">\r\n              <Text width=\"xs\" />\r\n            </Button>\r\n            <Button>\r\n              <Text width=\"sm\" />\r\n            </Button>\r\n          </div>\r\n        </CardContent>\r\n      </Card>\r\n    </div>\r\n  )\r\n}\r\n",
      "type": "registry:block",
      "target": "components/blocks/contact-form.tsx"
    }
  ]
}