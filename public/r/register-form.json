{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "register-form",
  "type": "registry:block",
  "title": "Register Form",
  "description": "Multi-step registration with tabs for personal and account information",
  "dependencies": [],
  "registryDependencies": [
    "button",
    "card",
    "input",
    "label",
    "text",
    "checkbox",
    "tabs",
    "icons"
  ],
  "files": [
    {
      "path": "registry/new-york/blocks/register-form.tsx",
      "content": "'use client'\r\n\r\nimport { Button } from '@/registry/new-york/ui/button'\r\nimport {\r\n  Card,\r\n  CardContent,\r\n  CardDescription,\r\n  CardHeader,\r\n  CardTitle,\r\n} from '@/registry/new-york/ui/card'\r\nimport { Input } from '@/registry/new-york/ui/input'\r\nimport { Label } from '@/registry/new-york/ui/label'\r\nimport { Text } from '@/registry/new-york/ui/text'\r\nimport { Checkbox } from '@/registry/new-york/ui/checkbox'\r\nimport { UserIcon } from '@/registry/new-york/icons'\r\nimport { LockIcon } from '@/registry/new-york/icons/lock'\r\nimport { MailCheckIcon } from '@/registry/new-york/icons/mail-check'\r\nimport { Tabs, TabsContent, TabsList, TabsTrigger } from '@/registry/new-york/ui/tabs'\r\n\r\nexport function RegisterForm() {\r\n  return (\r\n    <div className=\"flex min-h-[700px] w-full items-center justify-center p-4\">\r\n      <Card className=\"w-full max-w-md\">\r\n        <CardHeader className=\"space-y-1\">\r\n          <CardTitle>\r\n            <Text width=\"lg\" emphasis=\"primary\" />\r\n          </CardTitle>\r\n          <CardDescription>\r\n            <Text width=\"xl\" color=\"muted\" />\r\n          </CardDescription>\r\n        </CardHeader>\r\n        <CardContent>\r\n          <Tabs defaultValue=\"personal\" className=\"w-full\">\r\n            <TabsList className=\"grid w-full grid-cols-2\">\r\n              <TabsTrigger value=\"personal\">\r\n                <Text width=\"xs\" />\r\n              </TabsTrigger>\r\n              <TabsTrigger value=\"account\">\r\n                <Text width=\"xs\" />\r\n              </TabsTrigger>\r\n            </TabsList>\r\n            <TabsContent value=\"personal\" className=\"space-y-4\">\r\n              <div className=\"space-y-2\">\r\n                <Label htmlFor=\"firstname\">\r\n                  <Text width=\"xs\" />\r\n                </Label>\r\n                <Input\r\n                  id=\"firstname\"\r\n                  skeleton\r\n                  skeletonIcon={<UserIcon size={16} className=\"text-muted-foreground\" />}\r\n                />\r\n              </div>\r\n              <div className=\"space-y-2\">\r\n                <Label htmlFor=\"lastname\">\r\n                  <Text width=\"xs\" />\r\n                </Label>\r\n                <Input\r\n                  id=\"lastname\"\r\n                  skeleton\r\n                  skeletonIcon={<UserIcon size={16} className=\"text-muted-foreground\" />}\r\n                />\r\n              </div>\r\n              <div className=\"space-y-2\">\r\n                <Label htmlFor=\"email-reg\">\r\n                  <Text width=\"xs\" />\r\n                </Label>\r\n                <Input\r\n                  id=\"email-reg\"\r\n                  skeleton\r\n                  skeletonIcon={<MailCheckIcon size={16} className=\"text-muted-foreground\" />}\r\n                />\r\n              </div>\r\n            </TabsContent>\r\n            <TabsContent value=\"account\" className=\"space-y-4\">\r\n              <div className=\"space-y-2\">\r\n                <Label htmlFor=\"password-reg\">\r\n                  <Text width=\"xs\" />\r\n                </Label>\r\n                <Input\r\n                  id=\"password-reg\"\r\n                  skeleton\r\n                  skeletonIcon={<LockIcon size={16} className=\"text-muted-foreground\" />}\r\n                />\r\n              </div>\r\n              <div className=\"space-y-2\">\r\n                <Label htmlFor=\"confirm-password\">\r\n                  <Text width=\"sm\" />\r\n                </Label>\r\n                <Input\r\n                  id=\"confirm-password\"\r\n                  skeleton\r\n                  skeletonIcon={<LockIcon size={16} className=\"text-muted-foreground\" />}\r\n                />\r\n              </div>\r\n              <div className=\"flex items-start space-x-2\">\r\n                <Checkbox id=\"terms\" />\r\n                <Label htmlFor=\"terms\" className=\"cursor-pointer leading-none\">\r\n                  <div className=\"space-y-1\">\r\n                    <Text width=\"full\" size=\"sm\" />\r\n                    <Text width=\"lg\" size=\"xs\" color=\"muted\" />\r\n                  </div>\r\n                </Label>\r\n              </div>\r\n            </TabsContent>\r\n          </Tabs>\r\n          <Button className=\"mt-6 w-full\">\r\n            <Text width=\"sm\" />\r\n          </Button>\r\n        </CardContent>\r\n      </Card>\r\n    </div>\r\n  )\r\n}\r\n",
      "type": "registry:block",
      "target": "components/blocks/register-form.tsx"
    }
  ]
}